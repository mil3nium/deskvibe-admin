<form id="venue_form">

    <!-- CONTACT INFO -->
    <h2>Contact Info</h2>
    <fieldset class="form-group row">
        <label for="name" class="col-sm-2 form-control-label">Company name</label>

        <div class="col-sm-10">
            <input type="text" class="form-control" id="name" placeholder="DeskVibe" value="<%= venue_active.name%>">
        </div>
    </fieldset>
    <fieldset class="form-group row">
        <label for="contact_name" class="col-sm-2 form-control-label">Contact name</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="contact_name" placeholder="Name" value="<%= venue_active.contact.name%>">
        </div>
    </fieldset>
    <fieldset class="form-group row">
        <label for="contact_email" class="col-sm-2 form-control-label">Contact email</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="contact_email" placeholder="Email" value="<%= venue_active.contact.email%>">
        </div>
    </fieldset>
    <fieldset class="form-group row">
        <label for="Address" class="col-sm-2 form-control-label">Address</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" id="search_text" placeholder="Search address" >
        </div>
        <div class="col-sm-2">
            <input type="text" class="form-control" id="search_country" placeholder="Country" value="Norway">
        </div>
        <div class="col-sm-2">
            <button type="button" class="btn btn-primary" id="search">Search</button>
        </div>
        <div class="col-sm-2"></div>
        <div class="col-sm-4">
            <input disabled type="text" class="form-control" id="street" placeholder="Address" value="<%= venue_active.location.street%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="street_number" placeholder="Number" value="<%= venue_active.location.street_number%>">
        </div>

        <div class="col-sm-2">
            <input disabled type="number" class="form-control" id="postal_code" placeholder="Post number" value="<%= venue_active.location.postal_code%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="postal_town" placeholder="City" value="<%= venue_active.location.postal_town %>">
        </div>
        <div class="col-sm-2"></div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="county" placeholder="County" value="<%= venue_active.location.county%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="state" placeholder="State" value="<%= venue_active.state%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="country" placeholder="Country" value="<%= venue_active.location.country%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="lat" placeholder="Latitude" value="<%= venue_active.location.lat%>">
        </div>
        <div class="col-sm-2">
            <input disabled type="text" class="form-control" id="lng" placeholder="Longitude" value="<%= venue_active.location.lng%>">
        </div>
    </fieldset>
    <button type="button" class="btn btn-primary col-sm-2" id="submit_info">Submit</button>
</form>

<div class="modal fade" id="listModal">
    <div class="modal-dialog" role="list">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">Search list</h4>
            </div>
            <div class="modal-body">
                <table class="table table-hover" id="modal-table">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Address</th>
                    </tr>
                    </thead>
                    <tbody id="search-results">
                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->